[FORMAT]

# Maximum number of characters on a single line.
max-line-length=120


[MESSAGES CONTROL]

disable=missing-function-docstring,
    missing-module-docstring,
    missing-class-docstring,
    too-few-public-methods,
    duplicate-code,
    too-many-nested-blocks,
    line-too-long,
    too-many-boolean-expressions,
    too-many-nested-blocks,
    line-too-long,

#    import-error

[MASTER]
ignore=env
ignore-patterns=^env.*
ignore-paths=^env.*$